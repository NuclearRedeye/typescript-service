{
  "name": "@nuclearredeye/typescript-service",
  "version": "1.2.18",
  "description": "A Typescript Service template that can be used as a seed for new projects.",
  "main": "./dist/bin/index.js",
  "type": "module",
  "files": [
    "dist/bin/**/*"
  ],
  "scripts": {
    "clean": "rm -r build dist || true",
    "distclean": "rm -r node_modules && npm run clean || true",
    "build": "tsc && rollup -c",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "docs": "typedoc --out ./dist/docs/ --exclude **/*.test.ts ./src/",
    "release": "standard-version",
    "start": "node --experimental-modules ./dist/bin/typescript-service.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuclearredeye/experiment.typescript-service.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuclearredeye/experiment.typescript-service/issues"
  },
  "homepage": "https://github.com/nuclearredeye/experiment.typescript-service#readme",
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.28",
    "@types/supertest": "^2.0.8",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.1",
    "rollup-plugin-terser": "^5.2.0",
    "standard-version": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.8",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@nuclearredeye/typescript-library": "^1.5.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
